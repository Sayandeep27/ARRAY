class Solution {
public:
    int majorityElement(vector<int>& nums) {
         
         int n=nums.size();
         int cnt=0;
         int candidate=-1;

         for(int i=0;i<n;i++)
         {
            if(cnt==0)
            {
                candidate=nums[i];
            }

            if(candidate==nums[i])
            {
                cnt++;
            }
            else cnt--;
         }


         cnt=0;

         for(int i=0;i<n;i++)
         {
            if(nums[i]==candidate) cnt++;
         }


         if(cnt>n/2) return candidate;

         return -1;
    }
};
